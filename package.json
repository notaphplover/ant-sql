    "@types/ioredis": "^4.0.10",
    "@types/jasmine": "^3.3.12",
    "@types/node": "^11.13.2",
    "jasmine": "^3.4.0",
    "nyc": "^13.3.0",
    "pg": "^7.9.0",
    "tslint": "^5.15.0",
